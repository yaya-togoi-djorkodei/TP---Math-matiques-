#include <stdio.h>

int main() {
    int n, i, j, est_premier;
    
    printf("Entrez un nombre entier N : ");
    scanf("%d", &n);
    
    if (n < 2) {
        printf("Il n'y a pas de nombres premiers pour N < 2.\n");
        return 0;
    }
    
    printf("Nombres premiers jusqu'à %d :\n", n);
    
    for (i = 2; i <= n; i++) {
        est_premier = 1; // On suppose que i est premier
        
        // Vérifier si i est divisible par un nombre entre 2 et i/2
        for (j = 2; j <= i/2; j++) {
            if (i % j == 0) {
                est_premier = 0; // i n'est pas premier
                break;
            }
        }
        
        if (est_premier == 1) {
            printf("%d ", i);
        }
    }
    
    printf("\n");
    return 0;
}
